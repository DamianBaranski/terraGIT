## Deinstall any tile listed completely (initializing and updating both Terrain and Objects)

git submodule deinit Objects/w050n00
git submodule deinit Objects/w050n50
git submodule deinit Objects/w050n60
git submodule deinit Objects/w050n70
git submodule deinit Objects/w050n80
git submodule deinit Objects/w050s10
git submodule deinit Objects/w050s20
git submodule deinit Objects/w050s30
git submodule deinit Objects/w050s60
git submodule deinit Objects/w050s70
git submodule deinit Objects/w050s80
git submodule deinit Objects/w050s90
git submodule deinit Terrain/w050n00
git submodule deinit Terrain/w050n50
git submodule deinit Terrain/w050n60
git submodule deinit Terrain/w050n70
git submodule deinit Terrain/w050n80
git submodule deinit Terrain/w050s10
git submodule deinit Terrain/w050s20
git submodule deinit Terrain/w050s30
git submodule deinit Terrain/w050s60
git submodule deinit Terrain/w050s70
git submodule deinit Terrain/w050s80
git submodule deinit Terrain/w050s9

git submodule update --progress Objects/w050n00
git submodule update --progress Objects/w050n50
git submodule update --progress Objects/w050n60
git submodule update --progress Objects/w050n70
git submodule update --progress Objects/w050n80
git submodule update --progress Objects/w050s10
git submodule update --progress Objects/w050s20
git submodule update --progress Objects/w050s30
git submodule update --progress Objects/w050s60
git submodule update --progress Objects/w050s70
git submodule update --progress Objects/w050s80
git submodule update --progress Objects/w050s90
git submodule update --progress Terrain/w050n00
git submodule update --progress Terrain/w050n50
git submodule update --progress Terrain/w050n60
git submodule update --progress Terrain/w050n70
git submodule update --progress Terrain/w050n80
git submodule update --progress Terrain/w050s10
git submodule update --progress Terrain/w050s20
git submodule update --progress Terrain/w050s30
git submodule update --progress Terrain/w050s60
git submodule update --progress Terrain/w050s70
git submodule update --progress Terrain/w050s80
git submodule update --progress Terrain/w050s90
