## Deinstall any tile listed completely (deinitializing and updating both Terrain and Objects)

git submodule deinit Objects/e000n50
git submodule deinit Objects/e000n60
git submodule deinit Objects/e010n50
git submodule deinit Objects/e010n60
git submodule deinit Objects/e010n70
git submodule deinit Objects/e020n60
git submodule deinit Objects/e020n70
git submodule deinit Objects/e030n60
git submodule deinit Objects/e030n70
git submodule deinit Terrain/e000n50
git submodule deinit Terrain/e000n60
git submodule deinit Terrain/e010n50
git submodule deinit Terrain/e010n60
git submodule deinit Terrain/e010n70
git submodule deinit Terrain/e020n60
git submodule deinit Terrain/e020n70
git submodule deinit Terrain/e030n60
git submodule deinit Terrain/e030n70

git submodule update --progress Objects/e000n50
git submodule update --progress Objects/e000n60
git submodule update --progress Objects/e010n50
git submodule update --progress Objects/e010n60
git submodule update --progress Objects/e010n70
git submodule update --progress Objects/e020n60
git submodule update --progress Objects/e020n70
git submodule update --progress Objects/e030n60
git submodule update --progress Objects/e030n70
git submodule update --progress Terrain/e000n50
git submodule update --progress Terrain/e000n60
git submodule update --progress Terrain/e010n50
git submodule update --progress Terrain/e010n60
git submodule update --progress Terrain/e010n70
git submodule update --progress Terrain/e020n60
git submodule update --progress Terrain/e020n70
git submodule update --progress Terrain/e030n60
git submodule update --progress Terrain/e030n70
