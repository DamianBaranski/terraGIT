git submodule deinit Objects/w040s40
git submodule deinit Objects/w060s40
git submodule deinit Objects/w070s40
git submodule deinit Objects/w080s40
git submodule deinit Objects/w090s40
git submodule deinit Objects/w180s40
git submodule deinit Terrain/e010s40
git submodule deinit Terrain/e020s40
git submodule deinit Terrain/e030s40
git submodule deinit Terrain/e070s40
git submodule deinit Terrain/e110s40
git submodule deinit Terrain/e120s40
git submodule deinit Terrain/e130s40
git submodule deinit Terrain/e140s40
git submodule deinit Terrain/e150s40
git submodule deinit Terrain/e170s40
git submodule deinit Terrain/w020s40
git submodule deinit Terrain/w040s40
git submodule deinit Terrain/w060s40
git submodule deinit Terrain/w070s40
git submodule deinit Terrain/w080s40
git submodule deinit Terrain/w090s40
git submodule deinit Terrain/w180s40

git submodule update --progress Objects/e010s40
