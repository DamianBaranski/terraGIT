## Deinstall any tile listed completely (initializing and updating both Terrain and Objects)

git submodule deinit Objects/w020n00
git submodule deinit Objects/w020n10
git submodule deinit Objects/w020n20
git submodule deinit Objects/w020n30
git submodule deinit Objects/w020n50
git submodule deinit Objects/w020n60
git submodule deinit Objects/w020n70
git submodule deinit Objects/w020n80
git submodule deinit Objects/w020s10
git submodule deinit Objects/w020s20
git submodule deinit Objects/w020s40
git submodule deinit Objects/w020s50
git submodule deinit Objects/w020s60
git submodule deinit Objects/w020s70
git submodule deinit Objects/w020s80
git submodule deinit Objects/w020s90
git submodule deinit Terrain/w020n00
git submodule deinit Terrain/w020n10
git submodule deinit Terrain/w020n20
git submodule deinit Terrain/w020n30
git submodule deinit Terrain/w020n50
git submodule deinit Terrain/w020n60
git submodule deinit Terrain/w020n70
git submodule deinit Terrain/w020n80
git submodule deinit Terrain/w020s10
git submodule deinit Terrain/w020s20
git submodule deinit Terrain/w020s40
git submodule deinit Terrain/w020s50
git submodule deinit Terrain/w020s60
git submodule deinit Terrain/w020s70
git submodule deinit Terrain/w020s80
git submodule deinit Terrain/w020s90

git submodule update --progress Objects/w020n00
git submodule update --progress Objects/w020n10
git submodule update --progress Objects/w020n20
git submodule update --progress Objects/w020n30
git submodule update --progress Objects/w020n50
git submodule update --progress Objects/w020n60
git submodule update --progress Objects/w020n70
git submodule update --progress Objects/w020n80
git submodule update --progress Objects/w020s10
git submodule update --progress Objects/w020s20
git submodule update --progress Objects/w020s40
git submodule update --progress Objects/w020s50
git submodule update --progress Objects/w020s60
git submodule update --progress Objects/w020s70
git submodule update --progress Objects/w020s80
git submodule update --progress Objects/w020s90
git submodule update --progress Terrain/w020n00
git submodule update --progress Terrain/w020n10
git submodule update --progress Terrain/w020n20
git submodule update --progress Terrain/w020n30
git submodule update --progress Terrain/w020n50
git submodule update --progress Terrain/w020n60
git submodule update --progress Terrain/w020n70
git submodule update --progress Terrain/w020n80
git submodule update --progress Terrain/w020s10
git submodule update --progress Terrain/w020s20
git submodule update --progress Terrain/w020s40
git submodule update --progress Terrain/w020s50
git submodule update --progress Terrain/w020s60
git submodule update --progress Terrain/w020s70
git submodule update --progress Terrain/w020s80
git submodule update --progress Terrain/w020s90
