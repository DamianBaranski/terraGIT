## Deinstall any tile listed completely (initializing and updating both Terrain and Objects)

git submodule deinit Objects/w170n00
git submodule deinit Objects/w170n10
git submodule deinit Objects/w170n20
git submodule deinit Objects/w170n50
git submodule deinit Objects/w170n60
git submodule deinit Objects/w170n70
git submodule deinit Objects/w170n80
git submodule deinit Objects/w170s10
git submodule deinit Objects/w170s20
git submodule deinit Objects/w170s70
git submodule deinit Objects/w170s80
git submodule deinit Objects/w170s90
git submodule deinit Terrain/w170n00
git submodule deinit Terrain/w170n10
git submodule deinit Terrain/w170n20
git submodule deinit Terrain/w170n50
git submodule deinit Terrain/w170n60
git submodule deinit Terrain/w170n70
git submodule deinit Terrain/w170n80
git submodule deinit Terrain/w170s10
git submodule deinit Terrain/w170s20
git submodule deinit Terrain/w170s70
git submodule deinit Terrain/w170s80
git submodule deinit Terrain/w170s90

git submodule update --progress Objects/w170n00
git submodule update --progress Objects/w170n10
git submodule update --progress Objects/w170n20
git submodule update --progress Objects/w170n50
git submodule update --progress Objects/w170n60
git submodule update --progress Objects/w170n70
git submodule update --progress Objects/w170n80
git submodule update --progress Objects/w170s10
git submodule update --progress Objects/w170s20
git submodule update --progress Objects/w170s70
git submodule update --progress Objects/w170s80
git submodule update --progress Objects/w170s90
git submodule update --progress Terrain/w170n00
git submodule update --progress Terrain/w170n10
git submodule update --progress Terrain/w170n20
git submodule update --progress Terrain/w170n50
git submodule update --progress Terrain/w170n60
git submodule update --progress Terrain/w170n70
git submodule update --progress Terrain/w170n80
git submodule update --progress Terrain/w170s10
git submodule update --progress Terrain/w170s20
git submodule update --progress Terrain/w170s70
git submodule update --progress Terrain/w170s80
git submodule update --progress Terrain/w170s90
